{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/client",
    "functions": {
        "api/index.js": {
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/dist/client/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api"
        }
    ]
}