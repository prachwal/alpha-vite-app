{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/client",
    "functions": {
        "api/index.js": {
            "maxDuration": 30
        },
        "api/auth.js": {
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/api/auth.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/dist/client/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api"
        }
    ],
    "crons": [],
    "env": {
        "NEXT_PUBLIC_VERCEL_URL": "@vercel-url"
    },
    "git": {
        "deploymentEnabled": {
            "main": true
        }
    }
}